                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Lynn Conway"
                    android:textColor="#8af82c"
                    android:textSize="25dp"
                    android:textStyle="italic"
                    android:layout_marginTop="10dp"
                    android:layout_marginRight="10dp"/>

                <TextView
                    android:layout_width="179dp"
                    android:layout_height="wrap_content"
                    android:text="Co-wrote the book on VLSI which lead to today's microprocessors."
                    android:textSize="20dp"
                    android:layout_marginTop="5dp"/>

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="1980s"
                android:layout_gravity="center"
                android:layout_marginTop="30dp"
                android:textColor="#3333ff"
                android:textSize="30dp"
                android:textStyle="bold"/>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:weightSum="2">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Readia Perlman"
                android:textColor="#8af82c"
                android:textSize="25dp"
                android:textStyle="italic"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="5dp"
                android:layout_gravity="center_vertical" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Invented Spanning Tree Protocol (STP) enabling the creation of the Internet."
                android:textSize="20dp"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="5dp"/>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="1990s"
                android:layout_gravity="center"
                android:layout_marginTop="30dp"
                android:textColor="#3333ff"
                android:textSize="30dp"
                android:textStyle="bold"/>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:weightSum="2">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_weight="1">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Anita Borg"
                    android:textColor="#8af82c"
                    android:textSize="25dp"
                    android:textStyle="italic"
                    android:layout_marginTop="10dp"
                    android:layout_marginLeft="5dp"
                    />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Founded Systers community for women computer scientists and co-founded the Grace Hopper Celebration of Women in Computing."
                    android:textSize="20dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginLeft="5dp"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_weight="1"
                android:layout_gravity="center_vertical">

                <TextView
                    android:layout_width="188dp"
                    android:layout_height="wrap_content"
                    android:text="Adele Goldberg"
                    android:textColor="#8af82c"
                    android:textSize="25dp"
                    android:textStyle="italic"
                    android:layout_marginTop="10dp"
                    android:layout_marginRight="10dp" />

                <TextView
                    android:layout_width="179dp"
                    android:layout_height="wrap_content"
                    android:text="Co-developer of Smalltalk and pioneer of graphically based user interfaces."
                    android:textSize="20dp"
                    android:layout_marginTop="5dp"/>

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="2000s"
                android:layout_gravity="center"
                android:layout_marginTop="30dp"
                android:textColor="#3333ff"
                android:textSize="30dp"
                android:textStyle="bold"/>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:weightSum="2">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_weight="1">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Deborah Estrin"
                    android:textColor="#8af82c"
                    android:textSize="25dp"
                    android:textStyle="italic"
                    android:layout_marginTop="10dp"
                    android:layout_marginLeft="5dp"
                    />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Pioneer in the field of embedded network sensing."
                    android:textSize="20dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginLeft="5dp"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_weight="1">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Helen Greiner"
                    android:textColor="#8af82c"
                    android:textSize="25dp"
                    android:textStyle="italic"
                    android:layout_marginTop="10dp"
                    android:layout_marginRight="10dp"/>

                <TextView
                    android:layout_width="179dp"
                    android:layout_height="wrap_content"
                    android:text="Co-founder of iRobot and one of the original designers of iRobot Roomba."
                    android:textSize="20dp"
                    android:layout_marginTop="5dp"/>

            </LinearLayout>

        </LinearLayout>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

        </TableRow>


